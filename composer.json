{
    "name": "skazza/wp-modular-plugin-core",
    "description": "Core for WP Modular, a Modular Plugin for WordPress.",
    "keywords": [
        "wp",
        "wordpress",
        "wp-plugins",
        "modular"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Skazza",
            "email": "marianoscazzariello@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "gnugat/nomo-spaco": "^0.4.3",
        "guzzlehttp/guzzle": "^7.8",
        "laminas/laminas-config": "^3.9",
        "league/flysystem": "^3.16",
        "symfony/cache": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/proxy-manager-bridge": "^6.3",
        "symfony/yaml": "^6.3",
        "timber/timber": "^1.22",
        "twig/twig": "^3.7.1",
        "vlucas/phpdotenv": "^5.5.0"
    },
    "autoload": {
        "psr-4": {
            "WPModular\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
