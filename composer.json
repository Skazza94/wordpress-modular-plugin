{
    "name": "skazza/wp-modular-plugin-core",
    "description": "Core for WP Modular, a Modular Plugin for WordPress.",
    "keywords": [
        "wp",
        "wordpress",
        "wp-plugins",
        "modular"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Skazza",
            "email": "marianoscazzariello@gmail.com"
        }
    ],
    "require": {
        "gnugat/nomo-spaco": "~0.4",
        "guzzlehttp/guzzle": "^6.3",
        "league/flysystem": "^1.0",
        "symfony/yaml": "^3.2",
        "timber/timber": "^1.3",
        "twig/twig": "^1.34",
        "vlucas/phpdotenv": "^2.4",
        "zendframework/zend-config": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "WPModular\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
